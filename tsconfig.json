{
	"extends": "astro/tsconfigs/strictest",
	"compilerOptions": {
		"baseUrl": ".",
		"lib": ["es2022", "dom", "dom.iterable"],
		"paths": {
			"@/*": ["src/*"]
		},
		"jsx": "react-jsx",
		"jsxImportSource": "react"
	},
	"include": [".astro/types.d.ts", "**/*"],
	"exclude": ["node_modules", "**/node_modules/*", ".vscode", "dist"]
}
